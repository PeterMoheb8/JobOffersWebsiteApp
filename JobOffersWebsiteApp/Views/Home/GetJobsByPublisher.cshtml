@model IEnumerable<JobOffersWebsiteApp.Models.JobsViewModel>

<br />
@foreach (var gr in Model)
{


    <h3 class="alert alert-success">@gr.JobTitle</h3>
    <table class="table table-bordered table-hover">

        <tr>
            <th>
                اسم الباحث
            </th>

            <th>
                نص الرسالة
            </th>
            <th>
                تاريخ الرسالة
            </th>
        </tr>


        @foreach (var item in gr.Items)
        {


    <tr>
        <td>
            @item.User.UserName
        </td>
        <td>
            @item.Meesage
        </td>
        <td>
            @item.ApplayDate 
        </td>
    </tr>

        }



    </table>


}